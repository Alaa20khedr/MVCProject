@model ApplicationRole
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>RoleDetails</h1>
<br />
<br>
<div class="row">
    <div class="col-6">
        <form asp-action="Details" asp-controller="Rules">

            <div class="form-group">
                <label asp-for="@Model.Name" class="form-label"></label>
                <input asp-for="@Model.Name" class="form-control" readonly>
                <span asp-validation-for="@Model.Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@Model.CreateAt" class="form-label"></label>
                <input asp-for="@Model.CreateAt" class="form-control" readonly>
                <span asp-validation-for="@Model.CreateAt" class="text-danger"></span>
            </div>
            <br />
            <a asp-action="Update" class="btn btn-primary" asp-route-id="@Model.Id">Update</a>
            <a asp-action="Index" class="btn btn-light">Back To List</a>
        </form>
    </div>
</div>
@section Scripts {
    <partial name="_validationScriptsPartial">
}


